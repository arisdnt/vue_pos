{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "VuePOS Indonesia",
  "version": "1.0.0",
  "identifier": "com.vuepos.indonesia",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "VuePOS Indonesia",
        "width": 1366,
        "height": 768,
        "minWidth": 1024,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": false,
        "alwaysOnTop": false,
        "center": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "wix": {
        "language": "en-US"
      },
      "nsis": {
        "languages": [
          "en-US",
          "id-ID"
        ],
        "displayLanguageSelector": true
      }
    }
  }
}